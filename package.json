{
  "name": "live_cache",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/live_cache/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "proto:generate": "protoc --ts_proto_out=. ./src/proto/MSG_VehicleMonitoring.proto --ts_proto_opt=esModuleInterop=true",
    "proto:watch": "chokidar \"**/*.proto\" -c \"npm run proto:generate\"",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/live_cache/test/jest-e2e.json",
    "typeorm": "yarn ts-node ./node_modules/typeorm/cli.js",
    "typeorm:generate-migration:default": "yarn run typeorm -d ./orm.config.ts migration:generate",
    "typeorm:run-migration:default": "yarn run typeorm -d ./orm.config.ts migration:run"
  },
  "dependencies": {
    "@apollo/server": "^4.7.1",
    "@grpc/grpc-js": "^1.8.14",
    "@grpc/proto-loader": "^0.7.7",
    "@liaoliaots/nestjs-redis": "^9.0.5",
    "@nestjs-modules/ioredis": "^1.0.1",
    "@nestjs-query/query-graphql": "^0.30.0",
    "@nestjs/apollo": "^11.0.5",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.2",
    "@nestjs/core": "^9.0.0",
    "@nestjs/graphql": "^11.0.5",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/microservices": "^9.4.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/schedule": "^2.2.2",
    "@nestjs/typeorm": "^10.0.0",
    "@protobuf-ts/plugin": "^2.9.0",
    "@types/passport-jwt": "^3.0.8",
    "class-transformer": "^0.5.1",
    "google-protobuf": "^3.21.2",
    "graphql": "^16.6.0",
    "graphql-subscriptions": "^2.0.0",
    "grpc-tools": "^1.12.4",
    "ioredis": "^5.3.2",
    "joi": "^17.9.2",
    "moment": "^2.29.4",
    "nestjs-redis": "^1.3.3",
    "net": "^1.0.2",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pbjs": "^0.0.14",
    "pg": "^8.11.1",
    "protobufjs": "^7.2.3",
    "protobufjs-cli": "^1.1.1",
    "protoc-gen-ts": "^0.8.6",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "ts-proto": "^1.147.3",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "29.5.0",
    "@types/node": "18.15.11",
    "@types/passport": "^1.0.12",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.5.0",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "^4.9.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/entity(|/.*)$": "<rootDir>/libs/entity/src/$1"
    }
  },
  "main": "index.js"
}
